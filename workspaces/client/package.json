{
  "name": "vagabond-app-client",
  "version": "0.0.0",
  "private": true,
  "msw": {
    "workerDirectory": "public"
  },
  "relay": {
    "src": "./src",
    "schema": "./schema.graphql",
    "language": "typescript",
    "eagerEsModules": true,
    "exclude": ["**/node_modules/**", "**/__mocks__/**", "**/__generated__/**"],
    "featureFlags": {
      "enable_relay_resolver_transform": true,
      "enable_client_edges": {
        "kind": "enabled"
      }
    }
  },
  "type": "module",
  "scripts": {
    "msw:init": "msw init public",
    "dev:vite": "vite",
    "dev:netlify": "netlify dev",
    "dev": "pnpm run dev:netlify",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "ci": "rome ci ./src/**.ts ./src/**.tsx",
    "lint": "rome check ./src/**.ts ./src/**.tsx",
    "lint:fix": "rome check --apply ./src/**.ts ./src/**.tsx",
    "lint:fix:unsafe": "rome check --apply-unsafe ./src/**.ts ./src/**.tsx",
    "format": "rome format --write ./src/**.ts ./src/**.tsx",
    "relay": "relay-compiler"
  },
  "dependencies": {
    "@clerk/clerk-js": "^4.54.2",
    "@clerk/clerk-react": "^4.23.1",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.5",
    "@hookform/resolvers": "^3.1.1",
    "@mui/icons-material": "^5.14.1",
    "@mui/material": "^5.14.2",
    "@mui/system": "^5.14.1",
    "i18next": "^23.4.1",
    "jotai": "^2.2.2",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.10",
    "react-hook-form": "^7.45.2",
    "react-i18next": "^13.0.2",
    "react-relay": "^15.0.0",
    "react-router-dom": "^6.14.2",
    "relay-runtime": "^15.0.0"
  },
  "devDependencies": {
    "@swc-jotai/debug-label": "^0.0.10",
    "@swc-jotai/react-refresh": "^0.0.8",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@types/react-relay": "^14.1.4",
    "@types/relay-runtime": "^14.1.12",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitejs/plugin-react-swc": "^3.3.2",
    "babel-plugin-relay": "^15.0.0",
    "jotai-devtools": "^0.6.0",
    "jsdom": "^22.1.0",
    "relay-compiler": "^15.0.0",
    "vagabond-app-backend": "*",
    "vite": "^4.4.7",
    "vite-plugin-relay": "^2.0.0"
  }
}
