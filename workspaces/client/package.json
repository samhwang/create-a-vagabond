{
  "name": "vagabond-app-client",
  "version": "0.0.0",
  "private": true,
  "msw": {
    "workerDirectory": "public"
  },
  "relay": {
    "src": "./src",
    "schema": "../backend/schema.graphql",
    "language": "typescript",
    "eagerEsModules": true,
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ],
    "featureFlags": {
      "enable_relay_resolver_transform": true,
      "enable_client_edges": {
        "kind": "enabled"
      }
    }
  },
  "type": "module",
  "scripts": {
    "msw:init": "msw init public",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint": "eslint --cache .",
    "lint:fix": "npm run lint -- --fix",
    "prettier": "prettier --cache --write .",
    "format": "npm run lint:fix && npm run prettier",
    "relay": "relay-compiler"
  },
  "dependencies": {
    "@clerk/clerk-js": "^4.28.0",
    "@clerk/clerk-react": "^4.11.5",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.5.8",
    "@hookform/resolvers": "^2.9.11",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "i18next": "^22.4.10",
    "jotai": "^2.0.2",
    "notistack": "^2.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.43.2",
    "react-i18next": "^12.2.0",
    "react-relay": "^14.1.0",
    "react-router-dom": "^6.8.1",
    "relay-runtime": "^14.1.0",
    "web-vitals": "^3.1.1"
  },
  "devDependencies": {
    "@swc-jotai/debug-label": "^0.0.6",
    "@swc-jotai/react-refresh": "^0.0.4",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-relay": "^14.1.3",
    "@types/relay-runtime": "^14.1.8",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^3.1.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "babel-plugin-relay": "^14.1.0",
    "jotai-devtools": "^0.2.0",
    "jsdom": "^21.1.0",
    "relay-compiler": "^14.1.0",
    "vagabond-app-backend": "*",
    "vite": "^4.1.4",
    "vite-plugin-relay": "^2.0.0"
  }
}
