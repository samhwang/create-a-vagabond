{
  "name": "vagabond-app-client",
  "version": "0.0.0",
  "private": true,
  "msw": {
    "workerDirectory": "public"
  },
  "type": "module",
  "scripts": {
    "msw:init": "msw init public",
    "dev": "pnpm run msw:init && vite",
    "prebuild": "rimraf dist",
    "build": "rimraf dist && vite build",
    "preview": "vite preview",
    "pretest": "rimraf coverage",
    "typecheck": "tsc",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:CI": "pnpm run test && pnpm run typecheck",
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "prettier": "prettier --cache --write .",
    "format": "pnpm run lint:fix && pnpm run prettier",
    "schema": "npx get-graphql-schema http://localhost:8788/.netlify/functions/graphql > ./data/schema.graphql",
    "relay": "relay-compiler"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.9.10",
    "@radix-ui/react-avatar": "^1.0.1",
    "@radix-ui/react-dropdown-menu": "^2.0.2",
    "@tanstack/react-query": "^4.23.0",
    "@tw-classed/react": "^1.4.0",
    "@twind/core": "^1.1.3",
    "@twind/preset-autoprefix": "^1.0.7",
    "@twind/preset-container-queries": "^1.0.7",
    "@twind/preset-tailwind": "^1.1.4",
    "@twind/preset-tailwind-forms": "^1.1.2",
    "@twind/preset-typography": "^1.0.7",
    "@twind/with-react": "^1.1.3",
    "graphql": "^16.6.0",
    "jotai": "^1.13.1",
    "jotai-devtools": "^0.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.42.1",
    "react-relay": "^14.1.0",
    "react-router-dom": "^6.7.0",
    "relay-runtime": "^14.1.0",
    "web-vitals": "^3.1.1"
  },
  "devDependencies": {
    "@swc-jotai/debug-label": "^0.0.6",
    "@swc-jotai/react-refresh": "^0.0.3",
    "@tanstack/react-query-devtools": "^4.23.0",
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-relay": "^14.1.3",
    "@types/relay-runtime": "^14.1.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "babel-plugin-relay": "^14.1.0",
    "get-graphql-schema": "^2.1.2",
    "jsdom": "^21.1.0",
    "relay-compiler": "^14.1.0",
    "vite": "^4.0.4",
    "vite-plugin-relay": "^2.0.0"
  }
}
