{
  "name": "vagabond-app-client",
  "version": "0.0.0",
  "private": true,
  "msw": {
    "workerDirectory": "public"
  },
  "relay": {
    "src": "./src",
    "schema": "./data/schema.graphql",
    "language": "typescript",
    "eagerEsModules": true,
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ],
    "featureFlags": {
      "enable_relay_resolver_transform": true,
      "enable_client_edges": {
        "kind": "enabled"
      }
    }
  },
  "type": "module",
  "scripts": {
    "msw:init": "msw init public",
    "dev": "npm run msw:init && vite",
    "prebuild": "rimraf dist",
    "build": "rimraf dist && vite build",
    "preview": "vite preview",
    "pretest": "rimraf coverage",
    "typecheck": "tsc",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:CI": "npm run test && npm run typecheck",
    "lint": "eslint --cache .",
    "lint:fix": "npm run lint --fix",
    "prettier": "prettier --cache --write .",
    "format": "npm run lint:fix && npm run prettier",
    "schema": "npx get-graphql-schema http://localhost:8788/.netlify/functions/graphql > ./data/schema.graphql",
    "relay": "relay-compiler"
  },
  "dependencies": {
    "@clerk/clerk-js": "^4.24.0",
    "@clerk/clerk-react": "^4.9.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@hookform/resolvers": "^2.9.10",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.6",
    "graphql": "^16.6.0",
    "i18next": "^22.4.9",
    "notistack": "^2.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-hook-form": "^7.42.1",
    "react-i18next": "^12.1.4",
    "react-relay": "^14.1.0",
    "react-router-dom": "^6.7.0",
    "relay-runtime": "^14.1.0",
    "web-vitals": "^3.1.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.20.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/react-relay": "^14.1.3",
    "@types/relay-runtime": "^14.1.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitejs/plugin-react-swc": "^3.0.1",
    "babel-plugin-relay": "^14.1.0",
    "get-graphql-schema": "^2.1.2",
    "jsdom": "^21.1.0",
    "relay-compiler": "^14.1.0",
    "vite": "^4.0.4",
    "vite-plugin-relay": "^2.0.0"
  }
}
