{
  "name": "vagabond-app-backend",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest watch",
    "lint": "eslint --cache .",
    "lint:fix": "npm run lint --fix",
    "prettier": "prettier --cache --write .",
    "format": "npm run lint:fix && npm run prettier && npm run prisma:format",
    "typecheck": "tsc",
    "netlify:dev": "cd .. && netlify functions:serve",
    "dev": "npm run prisma:generate && npm run netlify:dev",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma db push",
    "prisma:generate": "prisma generate",
    "prisma:studio": "prisma studio",
    "prisma:format": "prisma format",
    "seed": "tsx ./prisma/seed.ts"
  },
  "devDependencies": {
    "prisma": "^4.9.0"
  },
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.6.4",
    "@devoxa/prisma-relay-cursor-connection": "^2.2.2",
    "@netlify/functions": "^1.4.0",
    "@pothos/core": "^3.24.0",
    "@pothos/plugin-errors": "^3.10.0",
    "@pothos/plugin-prisma": "^3.40.1",
    "@pothos/plugin-relay": "^3.33.1",
    "@pothos/plugin-simple-objects": "^3.6.7",
    "@prisma/client": "^4.9.0",
    "graphql": "^16.6.0",
    "graphql-yoga": "^3.3.0"
  }
}
